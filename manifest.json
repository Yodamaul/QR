{
   "manifest_version":3,
   "name":"QR Tool",
   "description":"qr tool",
   "version":"1.0",
   "content_scripts":[
      {
         "matches":[
            "<all_urls>"
         ],
         "js":[
            "qr.js"
         ]
      }
   ],
   "browser_action":{
      "default_icon":"icon.png",
      "default_title":"QR Code"
   },
   "permissions":[
      "tabs"
   ]
}